
#include "system.h"
#include "altera_avalon_pio_regs.h"
#include "sys/alt_stdio.h"
#include <unistd.h>


int main(void) {
    while (1) {
        // Allume la LED
        IOWR_ALTERA_AVALON_PIO_DATA(LED_OUT_BASE, 0x01);

        // Attend 2 secondes

        // Éteint la LED
        IOWR_ALTERA_AVALON_PIO_DATA(LED_OUT_BASE, 0x00);

        // Attend 2 secondes avant de répéter le processus

    }

    return 0;
}
