
#include "system.h"
#include "altera_avalon_pio_regs.h"
#include "sys/alt_stdio.h"
#include <time.h>
#include <stdio.h>
#include <unistd.h>

int main(void) {
    // Allume la LED
    IOWR_ALTERA_AVALON_PIO_DATA(LED_OUT_BASE, 0x01);

    // Attend 2 secondes
    sleep(2);

    // Éteint la LED
    IOWR_ALTERA_AVALON_PIO_DATA(LED_OUT_BASE, 0x00);

    return 0;
}
