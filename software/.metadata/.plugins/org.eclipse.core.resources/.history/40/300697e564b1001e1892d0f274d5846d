#include "system.h"
#include "altera_avalon_pio_regs.h"
#include <time.h>

int main(void){

		int tab[4];
		time_t current_time=0;
		tab[0]= 001;
		tab[2]= 200;

for (int i=0; i< 2; i++)
	{
		IOWR_ALTERA_AVALON_PIO_DATA(LED_OUT_BASE,0x01);
		for (;(clock()-current_time)<(2 * CLOCKS_PER_SEC););
		IOWR_ALTERA_AVALON_PIO_DATA(LED_OUT_BASE,0x02);
	}



	return 0;
};
